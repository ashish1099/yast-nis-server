Dump	none
Write	.rc.system.START_YPXFRD "no" true
Write	.rc.system.START_YPSERV "no" true
Write	.rc.system.START_YPPASSWDD "no" true
Write	.rc.system nil true
Return	nil
Dump	slave
Write	.rc.system.START_YPXFRD "no" true
Write	.rc.system.START_YPPASSWDD "no" true
Write	.rc.system.START_YPSERV "yes" true
Write	.rc.system.START_PORTMAP "yes" true
Write	.rc.system.START_YPBIND "yes" true
Write	.rc.system.YP_DOMAINNAME "thedomain" true
Write	.rc.system.CREATE_YP_CONF "yes" true
Write	.rc.system.YP_SERVER "127.0.0.1" true
Write	.rc.system nil true
Execute	.target.bash "/sbin/SuSEconfig --nonewpackage --module ypclient" 0
Return	nil
Dump	master
Write	.rc.system.START_YPSERV "yes" true
Write	.rc.system.YP_DOMAINNAME "thedomain" true
Write	.rc.ypserver.YPPWD_SRCDIR "/etc" true
Write	.var.yp.makefile.MERGE_PASSWD "true" true
Write	.rc.system.START_PORTMAP "yes" true
Write	.rc.system.START_YPBIND "yes" true
Write	.rc.system.YP_DOMAINNAME "thedomain" true
Write	.rc.system.CREATE_YP_CONF "yes" true
Write	.rc.system.YP_SERVER "127.0.0.1" true
Write	.rc.system nil true
Write	.rc.ypserver nil true
Write	.var.yp.makefile nil true
Execute	.target.bash "/sbin/SuSEconfig --nonewpackage --module ypclient" 0
Return	nil
